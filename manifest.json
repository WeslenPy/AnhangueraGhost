{
    "manifest_version": 3,
    "name": "AnhangueraGhost",
    "version": "2.0",
    "description": "Extensão para automatizar a vizualização de aulas na anhanguera.",
    "permissions": ["scripting", "activeTab"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_title": "AnhangueraGhost"
    },
    "icons": {
        "16": "images/icon-48.png",
        "48": "images/icon-48.png",
        "128": "images/icon-48.png"
    }
  }
  